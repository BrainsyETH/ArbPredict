{
  "name": "polymarket-kalshi-arb-bot",
  "version": "0.1.0",
  "description": "Cross-platform arbitrage bot for Polymarket and Kalshi prediction markets",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dry-run": "TRADING_MODE=dry_run node dist/index.js",
    "start:live": "TRADING_MODE=live node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint src/",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts",
    "discover": "tsx src/scripts/auto-discover.ts",
    "discover:crypto": "tsx src/scripts/auto-discover.ts crypto",
    "discover:dry-run": "tsx src/scripts/auto-discover.ts --dry-run",
    "discover:preview": "tsx src/scripts/auto-discover.ts --preview"
  },
  "keywords": [
    "arbitrage",
    "polymarket",
    "kalshi",
    "prediction-markets",
    "trading-bot"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ethersproject/wallet": "^5.7.0",
    "axios": "^1.6.7",
    "dotenv": "^16.4.1",
    "ethers": "^5.7.2",
    "pg": "^8.11.3",
    "pino": "^8.18.0",
    "pino-pretty": "^10.3.1",
    "ws": "^8.16.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
